{
  "name": "snake.js",
  "version": "1.0.1",
  "description": "Cl√°ssico jogo da cobrinha feito com JS e canvas.",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurLobopro/snake.js.git"
  },
  "keywords": [
    "snake"
  ],
  "author": "Arthur Lobo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArthurLobopro/snake.js/issues"
  },
  "homepage": "https://github.com/ArthurLobopro/snake.js#readme",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "electron": "^13.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./build/icon32.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "snake.js",
            "setupExe": "${name}-${version}-setup.exe",
            "setupIcon": "./build/icon32.ico",
            "iconURL": "./build/icon32.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
